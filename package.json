{
  "name": "@co.mmons/js-intl",
  "version": "1.0.0",
  "author": "co.mmons",
  "private": false,
  "license": "MIT",
  "scripts": {
    "build": "node-utils-file-system-glob-delete \"dist/**/*\" && tsc && tsc -p tsconfig.esm.json && node-utils-file-system-glob-copy ./ dist package.json && node-utils-file-system-glob-copy src dist \"**/package.json\" && node-utils-file-system-glob-copy ./ dist \"messages/**/*\"",
    "install": "node-utils-npm-install-dist"
  },
  "bin": {
    "js-intl-google-sheet-importer": "dist/google-sheet-importer-cli.js"
  },
  "dependencies": {
    "@co.mmons/node-utils": "git+https://github.com/co-mmons/node-utils.git",
    "fs-extra": "^5.0.0",
    "htmlparser2": "^3.9.2"
  },
  "devDependencies": {
    "@types/intl-messageformat": "^1.3.0",
    "@types/intl-relativeformat": "git+https://github.com/co-mmons/types-intl-relativeformat.git",
    "@types/fs-extra": "^5.0.1",
    "@types/htmlparser2": "^3.7.31",
    "@types/node": "^9.6.0",
    "@types/yargs": "^11.0.0",
    "typescript": "~3.2.1"
  },
  "peerDependencies": {
    "@co.mmons/js-utils": "git+https://github.com/co-mmons/js-utils.git",
    "intl-messageformat": "^2.0.0",
    "intl-relativeformat": "~2.0.0",
    "yargs": "^11.0.0"
  },
  "main": "index.js",
  "module": "esm/index.js",
  "sideEffects": false
}
